package po;

import java.util.Date;

/**
 * 订单信息实体类
 * @author Administrator
 *
 */
public class ORDERINFO {
	private String orderinfo_id; //订单编号(自动生成)
	private String userid; //用户编号
	private int ostatue;  //订单状态
	private Date DATE; //下单时间(自动生成)
	private int ototal; //订单总金额
	
	public String getOrderinfo_id() {
		return orderinfo_id;
	}
	public void setOrderinfo_id(String orderinfo_id) {
		this.orderinfo_id = orderinfo_id;
	}
	public String getUserid() {
		return userid;
	}
	public void setUserid(String userid) {
		this.userid = userid;
	}
	public int getOstatue() {
		return ostatue;
	}
	public void setOstatue(int ostatue) {
		this.ostatue = ostatue;
	}
	public Date getDATE() {
		return DATE;
	}
	public void setDATE(Date dATE) {
		DATE = dATE;
	}
	public int getOtotal() {
		return ototal;
	}
	public void setOtotal(int ototal) {
		this.ototal = ototal;
	}
	public ORDERINFO(String orderinfo_id, String userid, int ostatue,
			Date dATE, int ototal) {
		super();
		this.orderinfo_id = orderinfo_id;
		this.userid = userid;
		this.ostatue = ostatue;
		DATE = dATE;
		this.ototal = ototal;
	}
	
	public ORDERINFO(){
		
	}
	

}

package po;

import java.util.Date;

/**
 * 留言实体类
 * @author Administrator
 *
 */
public class MESSAGE {
	private String messid; //编号(自动生成)
	private String messcontent; //内容
	private Date messtime; //发表时间(自动生成)
	private String userid;//发表留言用户编号
	public String getMessid() {
		return messid;
	}
	public void setMessid(String messid) {
		this.messid = messid;
	}
	public String getMesscontent() {
		return messcontent;
	}
	public void setMesscontent(String messcontent) {
		this.messcontent = messcontent;
	}
	
	public Date getMesstime() {
		return messtime;
	}
	public void setMesstime(Date messtime) {
		this.messtime = messtime;
	}
	public String getUserid() {
		return userid;
	}
	public void setUserid(String userid) {
		this.userid = userid;
	}
	public MESSAGE(String messid, String messcontent, Date messtime,
			String userid) {
		super();
		this.messid = messid;
		this.messcontent = messcontent;
		this.messtime = messtime;
		this.userid = userid;
	}
	
	public MESSAGE(){
		
	}
}
